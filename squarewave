import numpy

import pygame
import math
import time
import numpy as np
import matplotlib as plt
pygame.init()

WIDTH = 500
HEIGHT = 500

screen = pygame.display.set_mode((WIDTH, HEIGHT))

run = True


clicked = False
locations = []

while run:
    
    for event in pygame.event.get():
            
        if event.type == pygame.QUIT:
            run = False
            
    


        
        pygame.display.update()